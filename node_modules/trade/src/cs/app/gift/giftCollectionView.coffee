define [
  "marionette",
  "gift/giftCollection"
  "gift/gift"
  "gift/zerogift"
  "user/userModel"
], ((Marionette, GiftCollection, Gift, ZeroGiftView, UserModel) ->
  class GiftCollectionView extends Marionette.CollectionView
    collection : GiftCollection
    itemView : Gift
    emptyView : ZeroGiftView

    setUser : (currentUser) ->
      @user = currentUser

    onBeforeRender : ->
      for model in @collection.models
        model.isCurrentUser(@user)
)
